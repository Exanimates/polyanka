{
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 2,
  "projects": {
    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "prefix": "frontend",
      "targets": {
        "serve": {
          "executor": "nx:run-script",
          "options": {
            "script": "serve"
          }
        }
      }
    },
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend",
      "projectType": "application",
      "prefix": "backend",
      "targets": {
        "serve": {
          "executor": "nx:run-script",
          "options": {
            "script": "serve"
          }
        }
      }
    }
  }
}
